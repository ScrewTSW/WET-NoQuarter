// Simple next map viewer (delete the comments if you use this file)
// g_msg2 of the banner vars is used to show the maps regularly in game 
// so keep in mind if you write your banner texts

set d1 "set g_gametype 2 ; map oasis ; set nextmap vstr d2; set g_msg2 Next map: Battery"
set d2 "set g_gametype 2 ; map battery ; set nextmap vstr d3; set g_msg2 Next map: Gold Rush"
set d3 "set g_gametype 2 ; map goldrush ; set nextmap vstr d4; set g_msg2 Next map: Würzburg Radar"
set d4 "set g_gametype 2 ; map radar ; set nextmap vstr d5; set g_msg2 Next map: Rail Gun"
set d5 "set g_gametype 2 ; map railgun ; set nextmap vstr d6; set g_msg2 Next map: Fuel Dump"
set d6 "set g_gametype 2 ; map fueldump ; set nextmap vstr d1; set g_msg2 Next map: Siwa Oasis"

// Now we just write a chat cmd with all names into a var usefully for custom commands: "exec map_cycle"
// Warning: Split up the chat cmds - there is a limit!
set map_cycle "chat |Battery|Gold Rush|Würzburg Radar|;chat |Rail Gun|Siwa Oasis|"

vstr d1
