 levelshots/baserace_b3c_cc_automap
{
    nopicmip
    nocompress
    nomipmaps
    {
        clampmap levelshots/baserace_b3c_cc.tga
        depthFunc equal
        rgbGen identity
    }
}

 levelshots/baserace_b3c_cc_trans
{
