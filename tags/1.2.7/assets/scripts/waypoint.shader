models/multiplayer/flagpole/flagpole_waypoint
{
	{
		map models/multiplayer/flagpole/flag_clouds.tga
		rgbGen lightingDiffuse
		tcgen environment
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		alphagen wave sin .6 .5 .25 .25
	}	
}

