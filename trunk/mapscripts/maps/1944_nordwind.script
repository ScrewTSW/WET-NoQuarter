com_spawn
{
	spawn
	{
		wait 50
		trigger com_spawn setup
	}

	trigger setup
	{
		setstate com_spawn_allied default			
	}

	trigger nuetral
	{
		setstate com_spawn invisible
		setstate com_spawn_allied invisible				
	}

	trigger showaxis
	{
		setstate com_spawn default
		setstate com_spawn_allied invisible
	}

	trigger showallies
	{
		setstate com_spawn default
		alertentity com_spawn
		setstate com_spawn_allied default
	}
