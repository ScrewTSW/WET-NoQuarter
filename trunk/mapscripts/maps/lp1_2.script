// ================================================
// ============   Depot Main Gates   ==============
// ================================================

gates
{
	spawn
	{
		wait 400
		trigger gates setup

		constructible_class 3
		globalaccum 5 set 0	// built flag
	}

	trigger setup
	{
		setstate gates_materials default
		setstate gates_materials_clip default
		setstate gates_flag default
	}

	buildstart final
	{
		setstate gates_materials default
		setstate gates_materials_clip default
		setstate gates_flag default
	}

	built final
	{
		setstate gates_materials invisible
		setstate gates_materials_clip invisible
		setstate gates_flag invisible
		globalaccum 6 set 1

		//wm_announce "The Main Gates have been constructed."
		alertentity print_depot
		setstate gates_cm_marker default

	}

	decayed final
	{
		setstate gates_materials default
		setstate gates_materials_clip default
		setstate gates_flag default


	}

	death
	{
		globalaccum 6 set 0
		setstate gates_materials default
		setstate gates_materials_clip default
		setstate gates_flag default


		wm_announce "The Main Gates have been destroyed."
		alertentity print_depot2
		setstate gates_cm_marker invisible

	}

	trigger remove
	{
		setstate gates_toi invisible
		setstate gates_materials invisible
		setstate gates_materials_clip invisible
		setstate gates_flag invisible


		remove
	}
}



	


