wh_door_script
{
	spawn
	{
		wait 200
		constructible_class 3
	}

	
	death
	{
		setstate bomb_cm_marker default
		//wm_announce "The Weapons Hold Door has been breached!"
		alertentity print_weapons_hold
		
	}

}

wires_construct
{
	spawn 
	{ 
		wait 200 
		constructible_class 3
		trigger self startup 
	} 

	buildstart final
	{
		setstate wires_broken invisible
	} 

	built final
	{
		setstate wires_broken invisible 
		setstate wires_construct invisible
		setstate wires_repaired default
		setstate fuse_box invisible
		setstate slide_doors_print invisible
		kill sparks_start
		kill fuse_noise_sparks
		alertentity fuse_smoke
		alertentity fuse_noise
		trigger slide_left open
	} 

	decayed final
	{ 
		trigger self startup 
	}

	trigger startup 
	{ 
		setstate wires_repaired invisible
		setstate wires_construct invisible 
		setstate wires_broken default 
	} 
}

