abomb_const
{
	spawn
	{
		wait 400
	}

	buildstart final
	{
		setstate bomb_built underconstruction
	}

	built final
	{
		kill opencrates
		setstate open_crates invisible
		kill bombbuild_toi1
		setstate bombbuild_toi2 invisible
		//wm_announce "The Bomb has been assembled"
		setstate bomb_built default
		setstate allied_hctrigger default
		setstate axis_hctrigger default
		setstate lb_crates_clip invisible	//turn off crates clip
		setstate bomb_cm_marker2 invisible
		alertentity print_bomb_built
	}

	decayed final
	{
		setstate bomb_built invisible
	}

	death
	{
	}
}





// ================================================
// =============   Hand Car Script  ===============
// ================================================


hc_car
{
	spawn
	{
		wait 200 wait
		followspline 0 car0 50000 wait
		accum 3 set 0
	}

	trigger handcar_pumpon
	{
		startanimation 1 19 20 nolerp
	}

	trigger handcar_pumpoff
	{
		startanimation 1 1 1 nolerp
	}

	trigger end_sequence	// here is the end, the allies have made it all the way.
	{
		setstate allied_hctrigger invisible	// remove the triggers no the teams can't
		setstate axis_hctrigger invisible	// effect the cars in anyway.
		setstate nogo_tm invisible
		trigger gates remove
		trigger self effects_off

		// this should end the round.
		trigger game_manager checkgame		
	}


	trigger disable
	{
		accum 9 set 0
		accum 1 abort_if_bitset 2
		accum 1 abort_if_bitset 8
		accum 1 bitset 8


		accum 1 bitreset 6
		accum 1 bitreset 7
		accum 1 bitreset 10
		accum 1 bitreset 11
		trigger self effects_off
	}

	trigger effects_off
	{
		trigger hc_car stop_sound
		trigger hc_car handcar_pumpoff

		trigger hc_fwheel stop
		trigger hc_rwheel stop
		trigger fb_fwheel stop
		trigger fb_rwheel stop
	}

	trigger enable_allies
	{
		accum 9 set 2
		accum 1 abort_if_bitset 6
		accum 1 abort_if_bitset 2
		accum 1 bitset 6
		accum 1 bitreset 8

		trigger hc_car allies_continue		
	}

	trigger enable_axis
	{

		accum 9 set 1
		accum 1 abort_if_bitset 7
		accum 1 abort_if_bitset 2
		accum 1 bitset 7
		accum 1 bitreset 8

		trigger hc_car axis_continue		
	}

	trigger allies_continue
	{
		trigger hc_car allies_sound
		trigger hc_car update_counter_allies
		trigger hc_car dispatch_forward
	}	

	trigger axis_continue
	{
		trigger hc_car axis_sound
		trigger hc_car update_counter_axis
		trigger hc_car dispatch_backward
	}
	
	trigger run_continue
	{
		accum 9 trigger_if_equal 2 hc_car allies_continue
		accum 9 trigger_if_equal 1 hc_car axis_continue
	}

	trigger check_gate
	{
		globalaccum 6 abort_if_equal 0
		trigger hc_car nogo2
	}

	trigger update_counter_allies
	{

		accum 3 inc 1
		accum 3 trigger_if_equal 38 hc_car check_gate
	}

	trigger update_counter_axis
	{
		accum 3 inc -1
		accum 3 abort_if_not_equal -2
		accum 3 set -1
	}

	trigger cancel_allies
	{
		trigger hc_car disable
		resetscript
	}

	trigger nogo1	//axis
	{
		// accum 3 set 0
		trigger self effects_off
		resetscript
	}

	trigger nogo2	//allied
	{
		accum 3 inc -1
		trigger self effects_off
		resetscript
	}

	trigger allies_sound
	{
		accum 1 abort_if_bitset 11
		accum 1 bitset 11
		trigger hc_car set_forward
		trigger hc_car start_sound
		trigger hc_car handcar_pumpon
	}

	trigger axis_sound
	{
		accum 1 abort_if_bitset 10
		accum 1 bitset 10
		trigger hc_car set_backward
		trigger hc_car start_sound
		trigger hc_car handcar_pumpon
	}

	trigger start_sound
	{
		playsound sound/vehicles/tug/tug_move.wav looping
		trigger fb_car start_sound
	}

	trigger stop_sound
	{
		stopsound
		trigger fb_car stop_sound
	}

	trigger set_forward
	{

		trigger hc_fwheel forward
		trigger hc_rwheel forward
		trigger fb_fwheel forward
		trigger fb_rwheel forward
	}

	trigger set_backward
	{

		trigger hc_fwheel backward
		trigger hc_rwheel backward
		trigger fb_fwheel backward
		trigger fb_rwheel backward
	}
		
	trigger dispatch_forward
	{
		accum 3 trigger_if_equal -1 hc_car run_continue
		accum 3 trigger_if_equal 0 hc_car run_continue
	
		accum 3 trigger_if_equal 1 hc_car runforwards_1
