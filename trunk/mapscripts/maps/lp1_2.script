game_manager
{
	spawn
	{
create
		{
			classname "team_WOLF_objective"
			scriptname "axis_spawn1"
			targetname "axis_spawn1"
			origin "-2498 -717 368"   // z = CC_FILTER_ALLIES(1) | CC_FILTER_SPAWNS(4)
			spawnflags 3	 // TEAM_AXIS(3)
			description "Axis Spawn"
		}

		// spawn1
		create
		{
			classname "team_CTF_redspawn"
			targetname "axis_spawn1"
			scriptName "axis_spawn1"
			origin "-2498 -717 368"
			spawnflags 3 // TEAM_AXIS
			angle "-90"
		}

		// spawn2
		create
		{
			classname "team_CTF_redspawn"
			targetname "axis_spawn1"
			scriptName "axis_spawn1"
			origin "-2670 -743 368"
			spawnflags 3 // TEAM_AXIS
			angle "-90"
		}
		// spawn3
		create
		{
			classname "team_CTF_redspawn"
			targetname "axis_spawn1"
			scriptName "axis_spawn1"
			origin "-3055 -1195 384"
			spawnflags 3 // TEAM_AXIS
			angle "-90"
		}


		create
		{
			classname "team_CTF_redspawn"
			targetname "axis_spawn1"
			scriptName "axis_spawn1"
			origin "3080 -1366 384"
			spawnflags 3 // TEAM_AXIS
			angle "90"
		}

		create
		{
			classname "team_CTF_redspawn"
			targetname "axis_spawn1"
			scriptName "axis_spawn1"
			origin "-3064 -1578 384"
			spawnflags 3 // TEAM_AXIS
			angle "90"
		}

		create
		{
			classname "team_CTF_redspawn"
			targetname "axis_spawn1"
			scriptName "axis_spawn1"
			origin "-3061 -1818 432"
			spawnflags 3 // TEAM_AXIS
			angle "90"
		}

		
		// Game rules
		wm_axis_respawntime	20
		wm_allied_respawntime	10
		wm_number_of_objectives 4
		wm_set_round_timelimit	25

		//spawn setup
		wait 50
		setautospawn    "Warehouse" 0
		setautospawn	"Allied Spawn"	1
		setstate        alliedsecondaryblob invisible
		alertentity 	axis_secondary
		
		//CP Setup
		wait 500
		setchargetimefactor 0 soldier 1
	

		//objective settings
		// 1: Steal Parts
		// 2: Build A-Bomb
		// 3: Escort Handcar
		// 4: Escape With Bomb
		// 5: Axis command post
		// 6: Allied command post
		


		// Current main objectives for each team (0=Axis, 1=Allies)
		wm_set_main_objective		1	0
		wm_set_main_objective		1	1

		wm_objective_status 1 0 0
		wm_objective_status 1 1 0
		wm_objective_status 2 0 0
		wm_objective_status 2 1 0
		wm_objective_status 3 0 0
		wm_objective_status 3 1 0
		wm_objective_status 4 0 0
		wm_objective_status 4 1 0

		wm_set_main_objective		1	0
		wm_set_main_objective		1	1

		// Stopwatch mode defending team (0=Axis, 1=Allies)
		wm_set_defending_team	0

		// Winner on expiration of round timer (0=Axis, 1=Allies)
		wm_setwinner	0

		

		//command map setup
		setstate bomb_cm_marker invisible
		setstate handcar_cm_toi invisible
	
		
							// fire at game start.
		wait 200
		setstate fuse_box invisible		// fusebox setup

		//handcar script info temporary
		wait 200
		globalaccum 5 set 0
		globalaccum 6 set 0
		setstate nogo_tm invisible
		setstate bomb_built invisible
		setstate gates_cm_marker invisible //turn off gates marker
		setstate bomb_cm_marker2 invisible //turn off bomb parts marker at LB

	}

	trigger checkgame
		{
		// Set the round winner:  0 == AXIS, 1 == ALLIED
		wm_setwinner 1
		// End the round
		wm_announce "Allied team has escaped with the A-Bomb!"
		wm_endround
		wait 500
		// allies have saved the day!
		}

	trigger timelimit_hit
		{
		wm_announce	"Axis have secured the Trainyard."
		wait 8000
		// what can we say.. axis pulled it off.. 
		}

}
	
