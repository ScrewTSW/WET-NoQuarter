hc_controller
{
	spawn
	{
		wait 800
	}

	trigger allied_request
	{
		globalaccum 1 inc 1
		globalaccum 0 set 0
		globalaccum 2 set 0
		globalaccum 1 abort_if_less_than 3
		globalaccum 5 set 0
		trigger hc_car enable_allies
	}

	trigger axis_request
	{
		globalaccum 0 inc 1
		globalaccum 1 set 0
		globalaccum 2 set 0
		globalaccum 0 abort_if_less_than 3
		globalaccum 5 set 0
		trigger hc_car enable_axis
	}

	trigger timer_disable
	{
		// trigger self allied_request	// test.. force the allied in.

		trigger self dual_disable
		globalaccum 2 inc 1
		globalaccum 2 abort_if_less_than 3
		globalaccum 0 set 0
		globalaccum 1 set 0
		trigger self disabler
	}

	trigger dual_disable
	{
		globalaccum 5 inc 1
		globalaccum 5 abort_if_less_than 3
		trigger self disabler
	}

	trigger disabler
	{
		trigger hc_car disable
	}
}

