dynamiteobj2
{
	spawn
	{
		wait 50				// Wait for all entities to spawn
		constructible_class 3			// Dynamite only
	}

	death
	{
		trigger forwardspawn1_flag force_allied 	// Switch forward spawn to Allied ONLY
