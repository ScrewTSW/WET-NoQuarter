		wm_teamvoiceannounce 1 "allies_hq_const_destroyed"
		
		
		setstate axisteamdoor1 invisible
		setstate axisteamdoor2 invisible
		setstate axisteamdoor3 invisible
		
		
		wm_objective_status 4 0 2
		wm_objective_status 4 1 1
	}
}




