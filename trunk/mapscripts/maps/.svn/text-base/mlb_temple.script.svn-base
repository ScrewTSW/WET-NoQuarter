game_manager
{
	spawn
	{		
		wm_axis_respawntime	15
		wm_allied_respawntime	10
		wm_number_of_objectives 6
		wm_set_round_timelimit	16		
		// Stopwatch mode defending team (0=Axis, 1=Allies)
		wm_set_defending_team	0
		// If the round timer expires, the Axis have won, so set the current winning team
		// Set the round winner:  0 == AXIS, 1 == ALLIED
		wm_setwinner 0		
		accum 1 set -1 // flag status
		accum 2 set 0

		wm_set_main_objective		1	0
		wm_set_main_objective		1	1

		// tank bridge
		wm_objective_status 1 0 0 
		wm_objective_status 1 1 0

		// tank barrier 1
		wm_objective_status 2 0 0 
		wm_objective_status 2 1 0

		// tank barrier 2
		wm_objective_status 3 0 0 
		wm_objective_status 3 1 0

		// temple door
		wm_objective_status 4 0 0 
		wm_objective_status 4 1 0

		// gold
		wm_objective_status 5 0 0 
		wm_objective_status 5 1 0

		// escape
		wm_objective_status 6 0 0 
		wm_objective_status 6 1 0

		wm_set_main_objective		1	0
		wm_set_main_objective		1	1

		wait 50

		setautospawn	"Allied Side"	1
		setautospawn	"Axis Spawn"	0
		setstate tele invisible
		setstate tele_pub invisible
		remapshader "sprites/objective" "textures/mlb_temple/objective_gold"		
		remapshaderflush		

		wait 1000

		// *----------------------------------- vo ------------------------------------------*
		wm_addteamvoiceannounce 0 "fueldump_axis_bridge_stop"
		wm_addteamvoiceannounce 0 "goldrush_axis_tankbars_construct"

		wm_addteamvoiceannounce 1 "fueldump_allies_bridge_construct"
		wm_addteamvoiceannounce 1 "ally-escorttank4"

		wm_teamvoiceannounce 0 "fueldump_axis_bridge_stop"
		wm_teamvoiceannounce 0 "goldrush_axis_tankbars_construct"
		wm_teamvoiceannounce 0 "axis_hq_compost_construct"

		wm_teamvoiceannounce 1 "fueldump_allies_bridge_construct"
		wm_teamvoiceannounce 1 "ally-escorttank4"
		// *---------------------------------------------------------------------------------*
	}

	trigger victory
	{
		wm_announce	"Allies have escaped with the gold crate"
		wm_objective_status 6 0 2 
		wm_objective_status 6 1 1
		wm_setwinner 1
		wait 1000
		wm_endround
	}

	trigger compost
	{
		accum 2 set 1
	}

	trigger approach
	{
		// *----------------------------------- vo ------------------------------------------*
		wm_teamvoiceannounce 0 "axis-tank1"

		wm_teamvoiceannounce 1 "ally-tanktemple"
		// *---------------------------------------------------------------------------------*
	}

	trigger bridge
	{
		wait 2500
		// *----------------------------------- vo ------------------------------------------*
		wm_teamvoiceannounce 0 "fueldump_axis_tank_bridge"

		wm_teamvoiceannounce 1 "ally-tankover"
		// *---------------------------------------------------------------------------------*
	}

	trigger flag1_firstred
	{
		accum 1 abort_if_not_equal -1
		accum 2 abort_if_not_equal 0
		accum 1 set 0
	}

	trigger flag1_firstblue
	{
		accum 1 abort_if_not_equal -1
		accum 1 set 1
		accum 2 abort_if_not_equal 0
		alertentity flagspawn
		alertentity flag1blob
		setautospawn	"Chruch Ruins"	1
		wm_announce	"^3Allies have captured the church ruins"
	}
	trigger flag1red
	{
		trigger game_manager flag1_firstred
		accum 1 abort_if_not_equal 1
		accum 2 abort_if_not_equal 0
		alertentity flagspawn
		alertentity flag1blob
		accum 1 set 0
		setautospawn	"Allied Side"	1
		wm_announce	"^3Axis have reclaimed the church ruins"

	}
	trigger flag1blue
	{
		trigger game_manager flag1_firstblue
		accum 1 abort_if_not_equal 0
		accum 2 abort_if_not_equal 0
		alertentity flagspawn
		alertentity flag1blob
		accum 1 set 1
		setautospawn	"Chruch Ruins"	1
		wm_announce	"^3Allies have captured the church ruins"
	}
	
	trigger acc
	{		
		globalaccum 2 abort_if_not_equal 1
		globalaccum 3 abort_if_not_equal 2
		globalaccum 1 abort_if_not_equal 3		
		wm_announce	"..."	
		wait 5000
		globalaccum 2 abort_if_not_equal 1
		globalaccum 3 abort_if_not_equal 2
		globalaccum 1 abort_if_not_equal 3
		globalaccum 4 abort_if_not_equal 0
		globalaccum 1 set 0
		globalaccum 2 set 0
		globalaccum 3 set 0	
		wm_announce	"......"					
		setstate tele default
		wait 100
		setstate tele invisible	
	}
	trigger pub_passed
	{
		globalaccum 4 abort_if_not_equal 10		
		wait 1000		
		setstate tele_pub default
		wait 1000
		setstate tele_pub invisible		
	}	
	
}

