// Heavy door hinges
//////////////////////////////////////////////////////////////////////////////////////////////////
rein_script
{
	spawn
	{
		wait 200
		constructible_class 3
		setstate heavy_door_triggerarea default
    	setstate heavy_door default
    	setstate heavy_door_fake invisible
	}

	built final
	{
	    setstate heavy_hinges default
    	setstate mat_heavy invisible
    	setstate heavy_door_triggerarea invisible
    	setstate heavy_door invisible
    	setstate heavy_door_fake default
    	wm_announce   "^1Allied team has reinforced the heavy door!"
	}

	death
	{
		setstate heavy_hinges invisible
    	setstate mat_heavy default
		wm_announce	"^1Axis team has destroyed the heavy door reinforcement!"
		wait 5000
		setstate heavy_door default
		setstate heavy_door_triggerarea default
		setstate heavy_door_fake invisible


	}
	trigger kill
	{
		setstate heavy_hinges invisible
    	setstate mat_heavy invisible
    	setstate heavy_door_fake invisible
		remove
	}
}

