sm_xx0
{
	spawn
	{
		wait 200		
		accum 0 set 0 //enabled ?
		accum 1 set 1 //minutes
		accum 2 set 0 //tens of seconds
		accum 3 set 0 //ones of seconds
		accum 4 set 60 //total seconds
		accum 5 set 0
		trigger sm_xx0 sec00
		trigger sm_x0x sec00
		trigger sm_0xx min00
		trigger sm_c end0
		trigger sm_xx02 sec00
		trigger sm_x0x2 sec00
		trigger sm_0xx2 min00
		trigger sm_c2 end0
		trigger sm_xx0 down_x0x
		trigger sm_xx0 down_0xx
		trigger sm_xx0 down_xx0				
		setstate sm_timer invisible			
	}			
	trigger time_is_up
	{
		accum 0 set 0		
		trigger sm_xx0 sec00
		trigger sm_x0x sec00
		trigger sm_0xx min00
		trigger sm_xx02 sec00
		trigger sm_x0x2 sec00
		trigger sm_0xx2 min00
		trigger game_manager count_off		
		wait 500
		trigger sm_xx0 end_display		
		wait 500
				
	}	
	trigger end_display
	{
		trigger sm_xx0 end1
		trigger sm_x0x end1
		trigger sm_c end0
		trigger sm_0xx end1	
		trigger sm_xx02 end1
		trigger sm_x0x2 end1
		trigger sm_c2 end0
		trigger sm_0xx2 end1		
		wait 250
		trigger sm_xx0 end2
		trigger sm_x0x end2
		trigger sm_c end2
		trigger sm_0xx end2
		trigger sm_xx02 end2
		trigger sm_x0x2 end2
		trigger sm_c2 end2
		trigger sm_0xx2 end2				
		wait 250
		trigger self end_display				
	}
							
	trigger stop
	{
		accum 0 set 0
		wm_announce	"^6Countdown interrupted"		
	}	
	trigger start
	{
		accum 0 set 1		
		setstate sm_timer default
		trigger sm_xx0 down10trig		
		wm_announce	"^6Countdown activated"			
	}		
	trigger down10trig
	{
		accum 4 trigger_if_equal 0 sm_xx0 time_is_up
		accum 0 abort_if_not_equal 1
		accum 3 inc -1
		accum 4 inc -1		
		trigger sm_xx0 down_xx0	
		wait 1000
		trigger sm_xx0 down10trig
	}				
	trigger down_xx0
	{		
		accum 3 trigger_if_equal 9 sm_xx0 sec09
		accum 3 trigger_if_equal 8 sm_xx0 sec08
		accum 3 trigger_if_equal 7 sm_xx0 sec07
		accum 3 trigger_if_equal 6 sm_xx0 sec06
		accum 3 trigger_if_equal 5 sm_xx0 sec05
		accum 3 trigger_if_equal 4 sm_xx0 sec04
		accum 3 trigger_if_equal 3 sm_xx0 sec03
		accum 3 trigger_if_equal 2 sm_xx0 sec02
		accum 3 trigger_if_equal 1 sm_xx0 sec01
		accum 3 trigger_if_equal 0 sm_xx0 sec00
		accum 3 trigger_if_equal 9 sm_xx02 sec09
		accum 3 trigger_if_equal 8 sm_xx02 sec08
		accum 3 trigger_if_equal 7 sm_xx02 sec07
		accum 3 trigger_if_equal 6 sm_xx02 sec06
		accum 3 trigger_if_equal 5 sm_xx02 sec05
		accum 3 trigger_if_equal 4 sm_xx02 sec04
		accum 3 trigger_if_equal 3 sm_xx02 sec03
		accum 3 trigger_if_equal 2 sm_xx02 sec02
		accum 3 trigger_if_equal 1 sm_xx02 sec01
		accum 3 trigger_if_equal 0 sm_xx02 sec00		
		accum 3 trigger_if_equal 0 sm_xx0 down10reset	
		accum 3 trigger_if_equal 9 sm_xx0 down10carry
		accum 4 trigger_if_equal 60 sm_xx0 run60
