//////////////////////////////////////////////////////////////////////////////////////////////////
// The old shed spawn
//////////////////////////////////////////////////////////////////////////////////////////////////

forward_spawn
{
	spawn
	{
		wait 200
		setstate shed_two_axis invisible
		setstate radio_two_axis invisible
		setstate gen_two_axis invisible
		accum 0 set 0
	}

	trigger forwardflagblue
	{
		wm_announce	"^1Axis lost the old shed!"
	}

	trigger forwardflagred
	{
		wm_announce	"^1Axis captured the old shed!"
	}

	trigger axis_capture
	{
		trigger self forwardflagred
		trigger self setaxis
	}

	trigger allied_capture
	{
		trigger self forwardflagblue
		trigger self setallies
	}

	trigger setaxis
	{
		setstate shed_two_axis default
		setstate radio_two_ally invisible
		alertentity radio_spawn_ally
		setautospawn	"The old shed"	0
		setautospawn "Generator room" 1
		globalaccum 6 set 1
		accum 0 set 0
	}

	trigger setallies
	{
		globalaccum 6 abort_if_equal 0
		setstate shed_two_axis invisible
		setstate radio_two_ally default
		alertentity radio_spawn_ally
		setautospawn	"Radio bunker"	1
		globalaccum 6 set 0
		accum 0 set 1
	}

	trigger force_axis
	{
		globalaccum 6 abort_if_equal 1
		setstate shed_two_axis default
		alertentity shed_two_axis
		alertentity shed_spawn_axis
		setstate radio_two_ally invisible
		alertentity radio_spawn_ally

	}		
	
	trigger force
	{
		trigger self force_axis		
		setstate forward invisible
	}
}
//////////////////////////////////////////////////////////////////////////////////////////////////
// Amplifier pole
//////////////////////////////////////////////////////////////////////////////////////////////////
amp_pole
{
	spawn
	{
		wait 500
		alertentity amp_sound
		trigger box_solid invisible
		trigger box_trans visible
	}

	death
	{
		trigger box_solid visible
		trigger box_trans invisible
		alertentity amp_sound
		// *----------------------------------- vo ------------------------------------------*
		wm_removeteamvoiceannounce 0 "axis-takeamp1"
		wm_removeteamvoiceannounce 0 "axis-giveamp3"
		// *---------------------------------------------------------------------------------*
	}
}

box_trans
{
	trigger visible
	{
		setstate box_solid invisible
		setstate box_trans default
	}
	trigger invisible
	{
		setstate box_solid default
		setstate box_trans invisible
	}
}

box_solid
{
	trigger visible
	{
		setstate box_solid default
		setstate box_trans invisible
	}
	trigger invisible
	{
		setstate box_solid invisible
		setstate box_trans default
	}
}

//////////////////////////////////////////////////////////////////////////////////////////////////
// Amplifier flag
//////////////////////////////////////////////////////////////////////////////////////////////////

amplifier_flag
{
	trigger stolen
	{	
	wm_announce	"^1Axis has taken the amplifier"

		// *----------------------------------- vo ------------------------------------------*
		wm_addteamvoiceannounce 0 "axis-giveamp3"
		wm_teamvoiceannounce 0 "axis-giveamp3"
		wm_removeteamvoiceannounce 1 "ally-proamp3"
		// *---------------------------------------------------------------------------------*

	setstate amp_cmm invisible
	}
	trigger dropped
	{
	wm_announce	"^1Axis has dropped the amplifier"
	
		// *----------------------------------- vo ------------------------------------------*
		wm_removeteamvoiceannounce 0 "axis-giveamp3"
		wm_teamvoiceannounce 1 "ally-proamp3"
		// *---------------------------------------------------------------------------------*

	}

	trigger returned
	{	
	wm_announce	"^1The amplifier has been returned"

		// *----------------------------------- vo ------------------------------------------*
		wm_addteamvoiceannounce 0 "axis-takeamp1"
		wm_teamvoiceannounce 0 "axis-takeamp1"
		// *---------------------------------------------------------------------------------*

	setstate amp_cmm default
	}

	trigger captured
	{		
	wm_announce	"^1The amplifier has been taken to the relay-station"
	setstate amp_cmm invisible

		// *----------------------------------- vo ------------------------------------------*
		wm_removeteamvoiceannounce 0 "axis-takeamp1"
		wm_removeteamvoiceannounce 0 "axis-giveamp3"
		wm_removeteamvoiceannounce 1 "ally-proamp3"
		// *---------------------------------------------------------------------------------*

	wm_objective_status 7 0 1  // Amp
		wm_objective_status 7 1 2
	globalaccum 7 bitset 1
	trigger boat1 on
	}

}

//////////////////////////////////////////////////////////////////////////////////////////////////
// Generator
//////////////////////////////////////////////////////////////////////////////////////////////////

generator
{
	spawn
	{
    	wait 200
    	constructible_class 2
    	trigger self startup
    	accum 0 set 0
	}

	buildstart final
	{
	}

	built final
	{
	    setstate gengreen default
    	setstate genred invisible
    	alertentity gensound
    	alertentity genspark
    	wm_announce   "^1Axis team has fixed the generator!"
    	wm_objective_status 6 0 1  // Generator
		wm_objective_status 6 1 0
    	globalaccum 7 bitset 0
    	trigger boat1 on
    	accum 0 abort_if_equal 1
    	setstate gen_two_axis default
    	alertentity gen_spawn_axis
    	setstate bb_two_ally invisible
		alertentity bb_spawn_ally
		setstate radio_two_ally invisible
		setstate gen_two_ally invisible		
    	setautospawn "Generator room" 0
    	setautospawn "Beach" 1
    	trigger forward_spawn force
    	trigger axis_compost2_built_model shift_gen
    	accum 0 set 1
		// *----------------------------------- vo ------------------------------------------*
		wm_removeteamvoiceannounce 0 "axis-repgen2"
		wm_addteamvoiceannounce 1 "ally-damgen1"
		wm_teamvoiceannounce 1 "ally-damgen1"
		// *---------------------------------------------------------------------------------*

	}

	decayed final
	{

	}

	death
	{
		trigger game_manager check
		trigger self startup
		wm_announce   "^1Allied team has disabled the generator!"
		wm_objective_status 6 0 0  // Generator
		wm_objective_status 6 1 0
		globalaccum 7 bitreset 0
		trigger boat1 off		
		// *----------------------------------- vo ------------------------------------------*
		wm_addteamvoiceannounce 0 "axis-repgen2"
		wm_teamvoiceannounce 0 "axis-repgen2"
		wm_removeteamvoiceannounce 1 "ally-damgen1"
		// *---------------------------------------------------------------------------------*  	

	}

	trigger startup
	{
		setstate gengreen invisible
		setstate genred default
		alertentity gensound
		alertentity genspark
	}
}
