game_manager
{
	spawn
	{
		// Scenario information

		wm_axis_respawntime 15
		wm_allied_respawntime 25
		wm_number_of_objectives 8
		wm_set_round_timelimit 20
		wm_set_defending_team 1
		wm_setwinner 1

		// Objectives

		wm_objective_status 1 0 0  // Ladder
		wm_objective_status 1 1 0
		wm_objective_status 2 0 0  // Tower
		wm_objective_status 2 1 0
		wm_objective_status 3 0 0  // Bunker roof
		wm_objective_status 3 1 0
		wm_objective_status 4 0 0  // HQ Command post
		wm_objective_status 4 1 0
		wm_objective_status 5 0 0  // Generator bunker gate
		wm_objective_status 5 1 0
		wm_objective_status 6 0 0  // Generator
		wm_objective_status 6 1 0
		wm_objective_status 7 0 0  // Amp
		wm_objective_status 7 1 0
		wm_objective_status 8 0 0  // Beach Command Post
		wm_objective_status 8 1 0
		// Accums			
		globalaccum 7 bitreset 0 // axis has gen ?
		globalaccum 7 bitreset 1 // axis has amp ?
		globalaccum 7 bitreset 2 // axis has HQ ?
		globalaccum 7 bitreset 3 // axis has BCP ?			
		globalaccum 6 set 0 // axis has flag ?		
		// Startspawn
		wait 200
		setautospawn	"Radio bunker"	1
		setstate amp_cmm default
		setstate tele invisible
		setstate tele_pub invisible
		remapshader "sprites/objective" "textures/mlb_bayraid/objective_amp"
		remapshaderflush		
		wait 1800		
		// *----------------------------------- vo ------------------------------------------*
		wm_addteamvoiceannounce 0 "axis-con1bunk4"
		wait 2000
		wm_teamvoiceannounce 0 "axis-con1bunk4"
		// *---------------------------------------------------------------------------------*		
	}
	
	trigger end
	{
		wait 100
		alertentity endextrig
		wait 7000
		wm_endround
	}
	trigger timelimit_hit
	{
		trigger boat1 move
		trigger boat2 move
		trigger boat3 move
		wait 10000				
	}
	trigger count_off
	{
		wm_setwinner 0
		setautospawn "Open beach" 1
		trigger boat1 move
		trigger boat2 move
		trigger boat3 move
		wait 100
		wm_announce	"^1Artillery on beach ^1!^3!^1!^3!^1!^3!^1!^3!^1!^3!"
		trigger self end				
	}
////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////
	
	trigger acc
	{		
		globalaccum 2 abort_if_not_equal 3
		globalaccum 3 abort_if_not_equal 2
		globalaccum 1 abort_if_not_equal 1
		globalaccum 4 abort_if_not_equal 0
		globalaccum 1 set 0
		globalaccum 2 set 0
		globalaccum 3 set 0
		globalaccum 4 set 0
		wm_announce	"^1axs!"			
		wait 1000		
		setstate tele_pub default
		wait 100
		setstate tele_pub invisible	
	}
	trigger pub_passed
	{
		globalaccum 4 abort_if_not_equal 100
		wait 1000		
		setstate tele_pub default
		wait 1000
		setstate tele_pub invisible		
	}	
}
////////////////////////////////////////////////////////////////////////////////
acc1
{
	activate
	{
		globalaccum 1 inc 1
		trigger game_manager acc
		wait 10000
		globalaccum 1 set 0				
	}
}	
acc2
{
	activate
	{
		globalaccum 2 inc 1
		trigger game_manager acc
		wait 10000
		globalaccum 2 set 0				
	}
}	
acc3
{
	activate
	{
